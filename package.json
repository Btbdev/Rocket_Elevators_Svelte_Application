		"babel-jest": "^29.0.1",
		"babel-plugin-transform-vite-meta-env": "^1.0.3",
		"jest": "^29.0.1",
		"jest-environment-jsdom": "^29.0.1",
		"jest-transform-stub": "^2.0.0",
		"jsdom": "^20.0.0",
